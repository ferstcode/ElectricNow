
<% if current_user.client? %>
<h4><%= @electric.name %></h4>
<table class= "table">
    <thead>
        <tr>
            <th>Calificaciones</th>            
            <th>Comentarios</th>
        </tr>
    </thead>
    <tbody>
        <% @qualifications.each do |q| %>
        <tr>
            <td><%=q.calification%></td>            
            <td><%=q.detail%></td>
        </tr>
        <% end  %>
    </tbody>
</table>
