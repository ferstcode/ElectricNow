<%= form_with(model: [detection, quatation]) do |f| %>
  <div class="form-group">
    <%= f.label :detail, "Comentario" %>
    <%= f.text_field :detail, class:'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :image %><br />
    <%= f.drag_and_drop_file_field :image %>
  </div>
  <div class="form-group">
        <%= f.label :commune, "Comuna" %>
        <%= f.text_field :commune, class:'form-control' %>
      </div>
  <div class="form-group">
        <%= f.label :date, "Fecha disponible" %>
        <%= f.date_field :date, class:'form-control' %>
    </div>
    <div class="form-group">
        <%= f.label :date, "Hora disponible" %>
        <%= f.date_field :date, class:'form-control' %>
        </div>

  
  <%= f.submit "Enviar", class:'btn btn-primary btn-sm' %>
<% end %>
